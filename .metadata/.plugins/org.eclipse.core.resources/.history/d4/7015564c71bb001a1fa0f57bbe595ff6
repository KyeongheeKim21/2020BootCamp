package algorithmicProblemSolvingStrategies;

import java.util.Scanner;

public class No1912 {
	static final int NONE = -987654321;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int N = scan.nextInt();
		int[] num = new int[N];
		int[][] dp = new int[N][N];
		int max = NONE;
		for(int i = 0 ; i < N; i++) {
			int x = scan.nextInt();
			num[i] = x ;
		}
		scan.close();
		/**/
		for(int i = 0; i < N; i++) {
			for(int j = i; j < N; j++) {
				if(i == j) dp[i][j] = num[i];
				else dp[i][j] = dp[i][j-1] + num[j];
				if(max < dp[i][j]) max = dp[i][j];
			}
		}
		System.out.println(max);
	}

}
