package algorithmicProblemSolvingStrategies;

import java.util.Scanner;

public class No1912 {
	static final int NONE = -987654321;

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int N = scan.nextInt();
		int[] num = new int[N];
		long[][] dp = new long[N][N];
		long max = NONE;
		long sum = 0;
		for(int i = 0 ; i < N; i++) {
			int x = scan.nextInt();
			num[i] = x ;
		}
		scan.close();
		/**/
		for(int i = 0; i < N; i++) {
			for(int j = i; j < N; j++) {
				if(i == j) sum = num[i];
				else sum += num[j];
				if(max < sum) max = sum;
			}
		}
		System.out.println(max);
	}

}
